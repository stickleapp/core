providers:
    - Workbench\App\Providers\WorkbenchServiceProvider
    - Dclaysmith\LaravelCascade\LaravelCascadeServiceProvider
    - Dclaysmith\LaravelCascade\EventServiceProvider
    - Dclaysmith\LaravelCascade\ScheduleServiceProvider

migrations:
    - workbench/database/migrations

workbench:
    start: "/"
    welcom: true
    install: true
    health: false
    discovers:
        web: true
        api: true
        commands: true
        components: true
        views: true
    sync: []
    build:
        - asset-publish
        - db-wipe
        - migrate-fresh:
              --seed: true
              --seeder: Dclaysmith\LaravelCascade\Database\Seeders\DatabaseSeeder
    assets:
        - laravel-assets

env:
    QUEUE_CONNECTION: sync
    APP_BASE_PATH: /Users/dclaysmith/Projects/LaravelCascade/workbench
    DB_CONNECTION: pgsql
    DB_HOST: 127.0.0.1
    DB_PORT: 5432
    DB_DATABASE: workbench
    DB_USERNAME: root
    DB_PASSWORD:
